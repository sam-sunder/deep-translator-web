{
    "builds": [
      {
        "src": "translator_website/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "translator_website/wsgi.py"
      }
    ]
}
